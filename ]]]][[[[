#include <iostream>
#include<cmath>
using namespace std;

int f (int*arr, int first,int last)
{
	int min=arr[0];
	int max=arr[8];
	int temp;
	for(;first<=last;++first)
	{
	if (arr[first]<min)
	{min=arr[first];}
	return min;
	}
	for(;first<=last;++first)
	{
	if (arr[last]>max)
	{max=arr[last];}
	return max;
	}
	{
	int temp=arr[min];
        arr[min]=arr[max];
        arr[max]=temp;
}
return arr[min];
}
int main() {
	int array[9]{56,2,3,4,5,6,7,8,9};
	cout<<f(array,9,10);
	return 0;
}
