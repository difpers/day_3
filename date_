#include <iostream>
using namespace std;

struct Date
{
    int year;
    int month;
    int day;
    Date()=default;
    Date (int m_year, int m_month, int m_day)
    {
        year=m_year;
        month=m_month;
        day=m_day;
    }
};
int main()
{
    Date a(2018,12,1);
    std::cout<<a.year<<" "<<a.month<<" "<<a.day;
}
